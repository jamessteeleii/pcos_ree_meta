@article{mcmahon_muscular_2014,
  title = {Muscular Adaptations and Insulin-like Growth Factor-1 Responses to Resistance Training Are Stretch-Mediated: {{Length Modulates Adaptation}}},
  shorttitle = {Muscular Adaptations and Insulin-like Growth Factor-1 Responses to Resistance Training Are Stretch-Mediated},
  author = {McMahon, Gerard and Morse, Christopher I. and Burden, Adrian and Winwood, Keith and Onamb{\'e}l{\'e}, Gladys Leopoldine},
  year = {2014},
