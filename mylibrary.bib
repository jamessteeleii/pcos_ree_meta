@article{fisherEffectsBreakdownSet2016,
  title = {The {{Effects}} of {{Breakdown Set Resistance Training}} on {{Muscular Performance}} and {{Body Composition}} in {{Young Men}} and {{Women}}},
  author = {Fisher, James P. and Carlson, Luke and Steele, James},
