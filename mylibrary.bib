@article{bolgerCausalProcessesPsychology2019,
  title = {Causal Processes in Psychology Are Heterogeneous},
  author = {Bolger, Niall and Zee, Katherine S. and {Rossignac-Milon}, Maya and Hassin, Ran R.},
