@article{edinboroughComparisonEffectKettlebell2016,
  title = {A {{Comparison}} of the {{Effect}} of {{Kettlebell Swings}} and {{Isolated Lumbar Extension Training}} on {{Acute Torque Production}} of the {{Lumbar Extensors}}},
  author = {Edinborough, Luke and Fisher, James P. and Steele, James},
