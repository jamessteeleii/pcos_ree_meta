@article{politoModeratorsStrengthGains2021,
  title = {Moderators of Strength Gains and Hypertrophy in Resistance Training: {{A}} Systematic Review and Meta-Analysis},
  shorttitle = {Moderators of Strength Gains and Hypertrophy in Resistance Training},
  author = {Polito, Marcos D. and Papst, Rafael R. and Farinatti, Paulo},
  year = {2021},
  month = oct,
  journal = {Journal of Sports Sciences},
  volume = {39},
  number = {19},
