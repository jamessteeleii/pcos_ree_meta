@article{gerringWhatMakesConcept1999,
  title = {What {{Makes}} a {{Concept Good}}? {{A Criterial Framework}} for {{Understanding}}                     {{Concept Formation}} in the {{Social Sciences}}},
  shorttitle = {What {{Makes}} a {{Concept Good}}?},
  author = {Gerring, John},
  year = {1999},
  month = mar,
  journal = {Polity},
  volume = {31},
  number = {3},
  pages = {357--393},
  publisher = {The University of Chicago Press},
  issn = {0032-3497},
  doi = {10.2307/3235246},
  urldate = {2023-08-11},
  abstract = {Nowhere in the broad and heterogeneous work on concept formation has the question of conceptual utility been satisfactorily addressed. Goodness in concept formation, I argue, cannot be reduced to 'clarity,' to empirical or theoretical relevance, to a set of rules, or to the methodology particular to a given study. Rather, I argue that conceptual adequacy should be perceived as an attempt to respond to a standard set of criteria, whose demands are felt in the formation and use of all social science concepts: (1) familiarity, (2) resonance, (3) parsimony, (4) coherence, (5) differentiation, (6) depth, (7) theoretical utility, and (8) field utility. The significance of this study is to be found not simply in answering this important question, but also in providing a complete and reasonably concise framework for explaining the process of concept formation within the social sciences. Rather than conceiving of concept formation as a method (with a fixed set of rules and a definite outcome), I view it as a highly variable process involving trade-offs among these eight demands.}
}

