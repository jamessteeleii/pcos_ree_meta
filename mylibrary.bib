  pmcid = {PMC6090762},
  pmid = {30068338},
  keywords = {Cardiac Rehabilitation,Cardiovascular,Exercise referral schemes,Exercise Therapy,Female,Humans,Mental Disorders,Mental health,Mental Health,Musculoskeletal,Musculoskeletal Diseases,Patient Compliance,Physical activity,Public Health,Referral and Consultation,Time Factors,United Kingdom},
