  file = {C\:\\Users\\james\\Zotero\\storage\\FFP7UUPZ\\Massin - 2017 - Towards a definition of efforts.pdf;C\:\\Users\\james\\Zotero\\storage\\ERJYE92E\\doiLanding.html}
