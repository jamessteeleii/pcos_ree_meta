@article{michellRaschParadoxConjoint2014,
  title = {The {{Rasch}} Paradox, Conjoint Measurement, and Psychometrics: {{Response}} to {{Humphry}} and {{Sijtsma}}},
  shorttitle = {The {{Rasch}} Paradox, Conjoint Measurement, and Psychometrics},
  author = {Michell, Joel},
  year = {2014},
