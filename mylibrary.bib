@article{rossFroomeHisSKY2019,
  title = {``{{Froome}} with {{His SKY Bodyguards}}, {{Layers}} of {{Armour}}'': {{The}} `{{Sport}} Is {{War}}' {{Conceptual Metaphor}} in {{Grand Tour Cycling Commentary}}},
