@article{swintonTraditionalContemporaryApproaches2024,
  title = {Traditional and Contemporary Approaches to Mathematical Fitness-Fatigue Models in Exercise Science: {{A}} Practical Guide with Resources. {{Part II}}.},
  shorttitle = {Traditional and Contemporary Approaches to Mathematical Fitness-Fatigue Models in Exercise Science},
