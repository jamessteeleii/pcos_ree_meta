  author = {Santos, Wanderson Divino Nilo Dos and Vieira, Carlos A. and Bottaro, Martim and Nunes, Vit{\'o}ria A. and {Ramirez-Campillo}, Rodrigo and Steele, James and Fisher, James P. and Gentil, Paulo},
  year = {2021},
