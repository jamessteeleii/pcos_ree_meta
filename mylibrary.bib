@article{vantilburgImpossibleHypothesesEffectSize2023,
  title = {Impossible {{Hypotheses}} and {{Effect-Size Limits}}},
  author = {{van Tilburg}, Wijnand A. P. and {van Tilburg}, Lennert J. A.},
  year = {2023},
