@article{fisherIntraSubjectVariabilityKm2017,
  title = {Intra-{{Subject Variability}} of 5 Km {{Time Trial Performance Completed}} by {{Competitive Trained Runners}}},
  author = {Fisher, James and Clark, Thomas and {Newman-Judd}, Katherine and Arnold, Josh and Steele, James},
  year = {2017},
