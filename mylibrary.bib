@article{willingerEffectivenessStructuredPhysical2021,
  title = {Effectiveness of {{Structured Physical Activity Interventions Through}} the {{Evaluation}} of {{Physical Activity Levels}}, {{Adoption}}, {{Retention}}, {{Maintenance}}, and {{Adherence Rates}}: {{A Systematic Review}} and {{Meta-Analysis}}},
  shorttitle = {Effectiveness of {{Structured Physical Activity Interventions Through}} the {{Evaluation}} of {{Physical Activity Levels}}, {{Adoption}}, {{Retention}}, {{Maintenance}}, and {{Adherence Rates}}},
  author = {Willinger, Nadja and Steele, James and Atkinson, Lou and Liguori, Gary and Jimenez, Alfonso and Mann, Steve and Horton, Elizabeth},
