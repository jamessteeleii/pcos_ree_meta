@article{mouraFunctionalCapacityImproves2018,
  title = {Functional Capacity Improves In-Line with Neuromuscular Performance after 12~Weeks of Non-Linear Periodization Strength Training in the Elderly},
  author = {de Moura, Bruno Monteiro and Sakugawa, Raphael Luiz and Orssatto, Lucas Bet da Rosa and {de Lima}, Luis Antonio Pereira and Pinto, Ronei Silveira and Walker, Simon and Diefenthaeler, Fernando},
