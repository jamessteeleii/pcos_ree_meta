@article{fosterScientificDiscoveryIts2016,
  title = {Scientific Discovery and Its Role in Sports Science},
  author = {Foster, Carl and Anholm, James and Best, Thomas and Boullosa, Daniel and Cress, Maria and {de Koning}, Jos and Gon{\c c}alves, Carlos and Hahn, Chelsea and Lucia, Alejandro and Porcari, John and Pyne, David and {Rodriguez-Marroyo}, Jose and Seiler, Stephen},
