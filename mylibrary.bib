  keywords = {estimated VO2max,exercise testing,measured VO2max,rating of perceived capacity,RPC scale},
  file = {C\:\\Users\\james\\Zotero\\storage\\2BKINDMX\\Gjestvang et al. - 2017 - How is rating of perceived capacity related to VO2.pdf;C\:\\Users\\james\\Zotero\\storage\\7MF4N2XI\\e000232.html}
