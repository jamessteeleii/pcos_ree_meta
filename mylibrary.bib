@article{paganTaskspecificResistanceTraining2024,
  title = {Task-Specific Resistance Training Adaptations in Older Adults: Comparing Traditional and Functional Exercise Interventions},
  shorttitle = {Task-Specific Resistance Training Adaptations in Older Adults},
  author = {Pagan, Jason I. and Bradshaw, Bethany A. and Bejte, Brisilda and Hart, Jordan N. and Perez, Vanjeliz and Knowles, Kevan S. and Beausejour, Jonathan P. and Luzadder, Marc and Menger, Reed and Osorio, Carlos and Harmon, Kylie K. and Hanney, William J. and Wilson, Abigail T. and Stout, Jeffrey R. and Stock, Matt S.},
  year = {2024},
  journal = {Frontiers in Aging},
  volume = {5},
  pages = {1335534},
  issn = {2673-6217},
  doi = {10.3389/fragi.2024.1335534},
  abstract = {Muscle strength declines {$\sim$}3\% per year after the age of 70. Resistance training guidelines for older adults are often based on free-weight and machine exercises, which may be inaccessible and lack carryover to activities of daily living. We tested the hypothesis that resistance training adaptations in older adults are task-specific. Thirty adults (8 males, 22 females; mean age = 71 years) were randomly assigned to participate in 6~weeks of supervised, high-intensity resistance training (twice per week) utilizing free-weight and machine exercises (traditional) versus functional activities that were overloaded with a weighted vest (functional). Participants were thoroughly familiarized with the exercises and testing prior to beginning the study. Major outcome measures included assessments of functional performance, five-repetition maximum strength, isometric knee extensor force, and quadriceps muscle size. Physical activity and nutrition were monitored. The study results demonstrate that the magnitude of improvement within a given outcome was largely dependent on group assignment, with greater improvements in gait speed and the timed-up-and-go in the functional group, but 2-3{\texttimes} greater five repetition maximum strength improvements for the trap bar deadlift, leg press, and leg extension following traditional resistance training. Both groups showed improvements in isometric knee extensor force and muscle size, suggesting that some aspects of the observed adaptations were generic, rather than specific. Overall, these novel findings suggest that, among older adults, 1) resistance training adaptations exhibit a high degree of task specificity and 2) significant improvements in functional outcomes can be achieved with the use of a weighted vest.},
  langid = {english},
  pmcid = {PMC11091347},
  pmid = {38746477},
  keywords = {aging,dynapenia,neuromuscular,older adults,sarcopenia,specificity},
  file = {C:\Users\james\Zotero\storage\UFCZFVGW\Pagan et al. - 2024 - Task-specific resistance training adaptations in o.pdf}
}

@article{pagePRISMA2020Statement2021,
  title = {The {{PRISMA}} 2020 Statement: An Updated Guideline for Reporting Systematic Reviews},
